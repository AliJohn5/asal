    "version": "0.2.0",
    "configurations": [
        {
            "name": "LOCAL EMULATOR",
            "cwd": "flutter_with_firebase_emulator",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "USE_FIREBASE_EMU=true",
            ]
        },
        {
            "name": "asal",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "asal (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "asal (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}